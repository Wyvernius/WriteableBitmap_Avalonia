<Styles xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="using:Slidecrew_UI">
    <Design.PreviewWith>
        <Border Padding="20">
            <!-- Add Controls for Previewer Here -->
        </Border>
    </Design.PreviewWith>
  <Style Selector="Button">
    <Setter Property="Background" Value="{DynamicResource ResourceKey=ColorOne}"/>
	<Setter Property="Foreground" Value="White"/>
  </Style>
  <Style Selector="Button > ContentPresenter:pointerover">
    <Setter Property="Background" Value="{DynamicResource ResourceKey=ColorTwo}"/>
  </Style>
